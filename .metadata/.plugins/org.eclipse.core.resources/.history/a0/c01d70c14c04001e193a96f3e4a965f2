package daos;

import entidades.Pais;

public interface PaisDAO {
	public boolean insertar(Integer id, String descripcion);
	public boolean existe(Integer id);
	public boolean existe(String descripcion);
	public Pais buscar(Integer id);
	public Pais buscar(String descripcion);
	public boolean eliminar(Integer id);
	public boolean modificar(Integer id, String descripcion);
	
}
