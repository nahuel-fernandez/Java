package daos;

import java.util.List;

import entidades.Ciudad;



public interface LocacionDAO {

	public boolean insertar(Integer idpais, Object locacion);
	public boolean existe(Integer id);
	public boolean existe(String descripcion);
	public Object buscar(Integer id);
	public List<Object> buscar(String descripcion, Integer id);
	public boolean eliminar(Integer id);
	public boolean modificar(Integer id, String descripcion);
	boolean insertar(Integer idpais, Ciudad ciudad);
}
