package main;

import dtos.PaisDTO;
import entidades.Pais;
public class App 
{
    public static void main( String[] args )
    {
        PaisDTO paisDTO = new PaisDTO();
        try{
        	paisDTO.insertar(new Pais ("Argentina"));
        	System.out.println("Se insert√≥ el pais Argentina en la base de datos.");
        }
        catch(Exception e) {
        	System.out.println(e.getMessage());
        	e.printStackTrace();
        }
    }
}
