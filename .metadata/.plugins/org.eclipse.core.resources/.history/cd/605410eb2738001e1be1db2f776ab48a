package daos;

import entidades.Pais;

////Interface para manejar el CRUD en la tabla de paises
public interface PaisDAO {
	public boolean insertar(Pais pais) throws Exception;
	public boolean existe(Pais p);
	public Pais buscar(Pais p);
	public boolean eliminar(Pais p);
	public boolean modificar(Pais p);
	
}
